<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8.0.171"
      fx:controller="de.uni_passau.se.memory.Controller.NumberOfPlayersController">
    <children>
        <AnchorPane prefHeight="29.0" prefWidth="600.0" style="-fx-background-color: black;">
            <children>
                <Label layoutX="29.0" layoutY="3.0" prefHeight="23.0" prefWidth="96.0" styleClass="text15"
                       text="TEAM-19 MEMORY" textFill="WHITE" AnchorPane.bottomAnchor="3.0" AnchorPane.leftAnchor="29.0"
                       AnchorPane.rightAnchor="475.0" AnchorPane.topAnchor="3.0">
                    <padding>
                        <Insets left="10.0"/>
                    </padding>
                </Label>
                <Button layoutX="571.0" layoutY="2.0" mnemonicParsing="false" onMouseClicked="#CloseStage"
                        prefHeight="6.0" prefWidth="24.0" styleClass="closebtn" text="X" textFill="WHITE">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                </Button>
                <Button layoutX="547.0" layoutY="2.0" mnemonicParsing="false" onMouseClicked="#MinimizeStage"
                        prefHeight="23.0" prefWidth="24.0" styleClass="minimizebtn" text="_" textFill="WHITE">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                </Button>
                <Button fx:id="EasterEgg" layoutY="-3.0" mnemonicParsing="false" onAction="#eeClicked" prefHeight="29.0"
                        prefWidth="32.0" styleClass="Card">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                </Button>
            </children>
        </AnchorPane>
        <AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="Background2" stylesheets="@Style.css">
            <children>
                <Button layoutX="150.0" layoutY="145.0" mnemonicParsing="false" onAction="#twoPlayers" prefHeight="40.0"
                        prefWidth="300.0" style="-fx-background-color: transparent;" styleClass="Banner"
                        text="One Friend">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                    <padding>
                        <Insets top="5.0"/>
                    </padding>
                </Button>
                <Button layoutY="229.0" mnemonicParsing="false" onAction="#threePlayers" prefHeight="40.0"
                        prefWidth="300.0" style="-fx-background-color: transparent;" styleClass="Banner"
                        text="Two Friends">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                    <padding>
                        <Insets top="5.0"/>
                    </padding>
                </Button>
                <Button layoutX="307.0" layoutY="229.0" mnemonicParsing="false" onAction="#fourPlayers"
                        prefHeight="40.0" prefWidth="300.0" style="-fx-background-color: transparent;"
                        styleClass="Banner" text="Three Friends">
                    <cursor>
                        <Cursor fx:constant="HAND"/>
                    </cursor>
                    <padding>
                        <Insets top="5.0"/>
                    </padding>
                </Button>
                <AnchorPane prefHeight="83.0" prefWidth="600.0" styleClass="BannerMultiP">
                    <children>
                        <Label alignment="CENTER" prefHeight="83.0" prefWidth="600.0" styleClass="text30"
                               text="Adventure with friends" textFill="WHITE" AnchorPane.bottomAnchor="0.0"
                               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <padding>
                                <Insets top="10.0"/>
                            </padding>
                        </Label>
                    </children>
                </AnchorPane>
            </children>
        </AnchorPane>
    </children>
</VBox>
